<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%date %-5level [%10thread] %-40logger:%line{15} - %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="akka" level="DEBUG" additivity="false"/>
<!--    <logger name="scalikejdbc.ActiveSession" level="OFF" additivity="false"/>-->

    <root level="DEBUG">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>
